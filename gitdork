#!/bin/bash


#check

if [ "$1" == "" ]
then
    echo -e "\033[36mUsage:\033[m $0 domains.txt  "
    echo -e "\033[31m [ Must Be Login On Github ] \033[m  "

    exit
fi

#check


host=$1


for domains in $(cat $host);
do

